        },
        methods: {
            decrease: function (item) {
                if (item.count > 0) {
                    item.count--;
                } else {
                    item.count = 0;
                }
            },
            increase: function (item) {
                if (item.count >= 0) {
                    item.count++;
                }
            },
            addCart: function (products) {
                console.log(products);
                let data = {
                    productId:
                        amount:
                }
                fetch("/api/Cart/", {
                    body: JSON.stringify(data),
                    headers: {
                        'content-type': 'application/json'
                    },
                    method: 'POST',
                }
                },
            mounted: {
                BasketBallView: function () {
                    let self = this;
                    fetch("/Product/Basketball")
                        .then(function (response) { return response.json() })
                        .then(function (result) { self.BasketBall = result })
                },
                PoolView: function () {
                    let self = this;
                    fetch("/Product/Pool")
                        .then(function (response) { return response.json() })
                        .then(function (result) { self.Pool = result })
                },
                BadmintonView: function () {
                    let self = this;
                    fetch("/Product/Badminton")
                        .then(function (response) { return response.json() })
                        .then(function (result) { self.Badminton = result })
                },
                SquashView: function () {
                    let self = this;
                    fetch("/Product/Squash")
                        .then(function (response) { return response.json() })
                        .then(function (result) { self.BasketBall = result })
                },
                TableTennisView: function () {
                    let self = this;
                    fetch("/Product/TableTennis")
                        .then(function (response) { return response.json() })
                        .then(function (result) { self.BasketBall = result })
                }
            }
        })
</script>



